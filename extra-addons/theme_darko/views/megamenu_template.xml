        <template id="custom_mobile_header" inherit_id="website.template_header_mobile"
                  name="Add brand to mobile header menu">
            <xpath expr="//div[@id='top_menu_collapse_mobile']//div[contains(@class,'offcanvas-header')]//button[contains(@class,'btn-close')]"
                   position="before">
                <li class="me-auto">
                    <a data-name="Navbar Logo" href="/" class="navbar-brand logo">
                        <span role="img" aria-label="Logo of My Website" title="My Website" data-oe-model="website.menu"
                              data-oe-id="1" data-oe-field="logo" data-oe-type="image"
                              data-oe-expression="website.logo">
                            <img src="/web/image/website/1/logo/My%20Website?unique=eddae40" class="img img-fluid"
                                 width="95" height="40" alt="My Website" loading="lazy"/>
                        </span>
                    </a>
                </li>
            </xpath>
        </template>
