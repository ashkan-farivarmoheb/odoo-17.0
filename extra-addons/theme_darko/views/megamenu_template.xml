<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="menu_pricing" model="website.menu">
            <field name="name">Pricing</field>
            <field name="url">/odoo-pricing</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">26</field>
        </record>

        <record id="menu_resources-insight" model="website.menu">
            <field name="name">resources &amp; insights</field>
            <field name="url">/resources-insights</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">28</field>
        </record>

        <!--         Mega Menu for Solutions2 -->
        <record id="menu_services" model="website.menu">
            <field name="name">Services</field>
            <field name="url">/services</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">21</field>
            <field name="is_mega_menu">True</field>
            <field name="mega_menu_content" type="html">
                <section class="o_cc5 mega-menu">
                    <div class="container-fluid">
                        <div class="row py-5 menu-content">
                            <!-- Blank Space: Takes 3 columns on large screens only -->
                            <div class="col-lg-3 d-none d-lg-block"></div>

                            <!-- Content: Takes full width on screens smaller than large (lg) -->
                            <div class="col-lg-9 col-12">
                                <div class="row">
                                    <!-- SOLUTIONS Column (hidden on mobile) -->
                                    <div class="col-lg-4 col-md-6 col-12 d-none d-md-block">
                                        <aside class="menu-description">
                                            <a href="/services" class="h3">
                                                <h2 class="my-4">Services</h2>
                                            </a>
                                            <p class="my-2">Optimize your business operations with our tailored Odoo
                                                solutions. We specialize in providing comprehensive consulting, seamless
                                                implementation, and ongoing support to ensure your success.
                                            </p>
                                        </aside>
                                    </div>

                                    <!-- First Column -->
                                    <div class="col-lg-4 col-md-6 col-12">
                                        <div class="my-2">
                                            <a href="/services-consulting" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">consulting</span>
                                            </a>
                                        </div>
                                        <div class="my-2">
                                            <a href="/services-implementation" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image2" class="link-image"/>
                                                <span class="link-text">implementation</span>
                                            </a>
                                        </div>
                                        <div class="my-2">
                                            <a href="/services-upgrade" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">upgrade</span>
                                            </a>
                                        </div>
                                    </div>

                                    <!-- Second Column -->
                                    <div class="col-lg-4 col-md-6 col-12">
                                        <div class="my-2">
                                            <a href="/services-support" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">support</span>
                                            </a>
                                        </div>
                                        <div class="my-2">
                                            <a href="/services-audit" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">audit</span>
                                            </a>
                                        </div>
                                        <div class="my-2">
                                            <a href="/services-training" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">training</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr class="full-width-separator d-none d-md-block"/>
                        <div class="container-fluid">
                            <a href="/services" class="see_all_link h5">
                                <span class="see_all_link-text">Explore Services</span>
                            </a>
                        </div>
                    </div>
                </section>

            </field>
        </record>
