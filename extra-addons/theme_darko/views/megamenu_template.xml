                                    </div>

                                    <!-- Second Column -->
                                    <div class="col-lg-4 col-md-6 col-12">
                                        <div class="my-2">
                                            <a href="/odoo-integration" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">Odoo Integration</span>
                                            </a>
                                        </div>
                                        <div class="my-2">
                                            <a href="/odoo-licence" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">Odoo Licence</span>
                                            </a>
                                        </div>
                                        <div class="my-2">
                                            <a href="/" class="link-with-image h3">
                                                <img src="/theme_darko/static/src/img/icons_3/advanced-e-commerce-svg.svg"
                                                     alt="Image1" class="link-image"/>
                                                <span class="link-text">Code Audit</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr class="full-width-separator d-none d-md-block"/>
                        <div class="container-fluid">
                            <a href="/odoo-erp" class="see_all_link h5">
                                <span class="see_all_link-text">Explore Odoo ERP</span>
                            </a>
                        </div>

                    </div>
                </section>
            </field>
        </record>
