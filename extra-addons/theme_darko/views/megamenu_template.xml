<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="menu_pricing" model="website.menu">
            <field name="name">Pricing</field>
            <field name="url">/odoo-pricing</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">26</field>
        </record>

        <record id="menu_resources-insight" model="website.menu">
            <field name="name">resources &amp; insights</field>
            <field name="url">/resources-insights</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">28</field>
        </record>

        <!--         Mega Menu for Solutions2 -->
        <record id="menu_services" model="website.menu">
            <field name="name">Services</field>
            <field name="url">/services</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">21</field>
            <field name="is_mega_menu">True</field>
            <field name="mega_menu_content" type="html">
                <section class="mega-menu">
                    <div class="container-fluid">
                        <!-- Content: Takes full width on screens smaller than large (lg) -->
                        <div class="row d-flex flex-column flex-lg-row justify-content-center">
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/services-consulting" class="link-with-image">
                                        <span class="link-text">consulting</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/services-implementation" class="link-with-image">
                                        <span class="link-text">implementation</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/services-upgrade" class="link-with-image">

                                        <span class="link-text">upgrade</span>
                                    </a>
                                </div>

                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/services-support" class="link-with-image">

                                        <span class="link-text">support</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/services-audit" class="link-with-image">

                                        <span class="link-text">audit</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/services-training" class="link-with-image">

                                        <span class="link-text">training</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </div>
                </section>

            </field>
        </record>
        <record id="menu_odoo" model="website.menu">
            <field name="name">Odoo</field>
            <field name="url">/odoo-erp</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">22</field>
            <field name="is_mega_menu">True</field>
            <field name="mega_menu_content" type="html">
                <section class="mega-menu">
                    <div class="container-fluid">
                        <!-- Content: Takes full width on screens smaller than large (lg) -->
                        <div class="row d-flex flex-column flex-lg-row justify-content-center">
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/odoo-pricing" class="link-with-image">
                                        <span class="link-text">Odoo Pricing</span>
                                    </a>
                                </div>
                                <div class="col-auto">

                                    <div class="my-2">
                                        <a href="/odoo-implementation" class="link-with-image">
                                            <span class="link-text">Odoo Implementation</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/odoo-customisation" class="link-with-image">
                                            <span class="link-text">Odoo Customisation</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/odoo-migration" class="link-with-image">
                                            <span class="link-text">Odoo Migration</span>
                                        </a>
                                    </div>
                                    <!-- Add other items here -->
                                </div>
                            </div>

                            <!-- Second Column -->
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/odoo-integration" class="link-with-image">
                                        <span class="link-text">Odoo Integration</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/odoo-licence" class="link-with-image">
                                        <span class="link-text">Odoo Licence</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/" class="link-with-image">
                                        <span class="link-text">Code Audit</span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </div>
                </section>
            </field>
        </record>
        <record id="menu_industries" model="website.menu">
            <field name="name">Industries</field>
            <field name="url">/industries</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">23</field>
            <field name="is_mega_menu">True</field>
            <field name="mega_menu_content" type="html">
                <section class="mega-menu">
                    <div class="container-fluid">
                        <!-- Responsive Flex Direction: Column on small, Row on large -->
                        <div class="row d-flex flex-column flex-lg-row justify-content-center">


                            <!-- Content: Takes full width on screens smaller than large (lg) -->
                            <div class="col-auto">
                                <div class="my-2">
                                    <a href="/industries-ecommerce" class="link-with-image">
                                        <span class="link-text">Ecommerce</span>
                                    </a>
                                </div>
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/industries-startups" class="link-with-image">
                                            <span class="link-text">Startups</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/industries-professional-services" class="link-with-image">
                                            <span class="link-text">Professional Services</span>
                                        </a>
                                    </div>

                                </div>

                                <!-- Second Column -->
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/industries-food-beverages" class="link-with-image">
                                            <span class="link-text">Food &amp; Beverages</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/industries-finance" class="link-with-image">
                                            <span class="link-text">Finance</span>
                                        </a>
                                    </div>
                                </div>
                                <div class="col-auto">
                                    <div class="my-2">
                                        <a href="/industries-retail" class="link-with-image">
                                            <span class="link-text">Retail</span>
                                        </a>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </section>
            </field>
        </record>

