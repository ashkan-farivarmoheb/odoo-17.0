<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_delivery_carrier_form_inherit_auspost" model="ir.ui.view">
        <field name="name">delivery.carrier.form.inherit.auspost</field>
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page/group[@name='Custom Delivery']" position="inside">
                <group name="Australia Post Shipping Information" string="Australia Post Shipping Information">
                    <field name="australia_post_service_code"/>
                    <field name="australia_post_return_service_code"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
