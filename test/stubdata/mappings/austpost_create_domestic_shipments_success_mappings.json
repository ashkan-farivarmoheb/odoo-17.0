{
    "mappings": [
        {
            "request": {
                "method": "POST",
                "urlPath": "/shipping/v1/shipments",
                "bodyPatterns": [
                    {
                        "matchesJsonPath": {
                            "expression": "$.shipments[0].shipment_reference",
                            "matches": ".*"
                        }
                    }
                ]
            },
            "response": {
                "headers": {
                    "Content-Type": "application/json"
                },
                "status": 201,
                "bodyFileName": "austpost_create_domestic_shipments_success.json"
            }
        }
    ]
}
