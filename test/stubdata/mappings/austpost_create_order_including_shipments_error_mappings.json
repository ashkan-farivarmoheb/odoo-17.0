{
    "mappings": [
        {
            "request": {
                "method": "POST",
                "urlPath": "/shipping/v1/orders",
                "bodyPatterns": [
                    {
                        "matchesJsonPath": {
                            "expression": "$.order_reference",
                            "expression": "$.shipments[0].shipment_reference",
                            "matches": "invalidorder"
                        }
                    }
                ]
            },
            "response": {
                "headers": {
                    "Content-Type": "application/json"
                },
                "status": 400,
                "bodyFileName": "austpost_create_order_including_shipments_400_error.json"
            }
        }
    ]
}
