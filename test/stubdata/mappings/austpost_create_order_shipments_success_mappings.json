{
    "mappings": [
        {
            "request": {
                "method": "PUT",
                "urlPath": "/shipping/v1/orders",
                "bodyPatterns": [
                    {
                        "matchesJsonPath": "$[?(@.shipments.length() == 1)]"

                          
                        
                    }
                ]
            },
            "response": {
                "headers": {
                    "Content-Type": "application/json"
                },
                "status": 201,
                "bodyFileName": "austpost_create_order_shipments_with_one_shipment_success.json"
            }
        }, 
        {
            "request": {
                "method": "PUT",
                "urlPath": "/shipping/v1/orders",
                "bodyPatterns": [
                    {
                        "matchesJsonPath": "$[?(@.shipments.length() == 2)]"

                    }
                ]
            },
            "response": {
                "headers": {
                    "Content-Type": "application/json"
                },
                "status": 201,
                "bodyFileName": "austpost_create_order_shipments_with_two_shipment_success.json"
            }
        }
        
    ]
}
