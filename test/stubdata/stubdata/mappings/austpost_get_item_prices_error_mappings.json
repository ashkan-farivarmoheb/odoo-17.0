{
    "mappings": [
        {
            "request": {
                "method": "POST",
                "urlPath": "/shipping/v1/prices/items",
                "bodyPatterns": [
                    {
                        "and": [
                            {
                                "matchesJsonPath": {
                                "expression": "$.items[0].item_reference",
                                "matches": "invalid"
                                }
                            },
                            {
                                "matchesJsonPath": {
                                "expression": "$.items[0].length",
                                "matches": "1000"
                                }
                            }
                        ]

                    }
                ]
            },
            "response": {
                "headers": {
                    "Content-Type": "application/json"
                },
                "status": 400,
                "bodyFileName": "austpost_get_item_prices_400_error.json"
            }
        }
    ]
}
